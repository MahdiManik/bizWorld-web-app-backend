{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "company": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company.company"
    },
    "user_profile": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-profile.user-profile"
    },
    "phone": {
      "type": "string"
    },
    "fullName": {
      "type": "string",
      "required": false
    },
    "investerStatus": {
      "type": "enumeration",
      "default": "PENDING",
      "enum": [
        "PENDING",
        "INVESTER"
      ]
    },
    "consultantStatus": {
      "type": "enumeration",
      "default": "NONE",
      "enum": [
        "NONE",
        "PENDING",
        "CONSULTANT"
      ]
    },
    "consultantData": {
      "type": "json"
    },
    "userStatus": {
      "type": "enumeration",
      "required": true,
      "default": "PENDING",
      "enum": [
        "ACTIVE",
        "INACTIVE",
        "PENDING"
      ]
    },
    "documentCv": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    },
    "listings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::listing.listing",
      "mappedBy": "businessOwner"
    }
  }
}
